{
  "name": "digitalcnzz-talentsmanage-web",
  "version": "1.1.0",
  "description": "talents manage",
  "author": "admin@digitalcnzz.com",
  "private": true,
  "license": "ISC",
  "scripts": {
    "start:dev": "cross-env API_ENV=dev MOCK=none UMI_UI=none umi dev",
    "build:test": "cross-env API_ENV=test umi build",
    "build:prod": "cross-env API_ENV=prod umi build",
    "analyze": "cross-env API_ENV=prod ANALYZE=1 umi build",
    "postinstall": "umi g tmp",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "tsc": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^5.0.8",
    "@ant-design/pro-table": "2.2.9",
    "antd": "4.2.5",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "bizcharts": "^3.4.3",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "qiniu-js": "^2.5.5",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.8.6",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "umi": "^3.2.0",
    "umi-request": "^1.3.2",
    "use-merge-value": "^1.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.18",
    "@umijs/fabric": "^2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.0.9",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "compression-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "md5": "^2.3.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^3.0.0",
    "rc-form": "^2.4.11",
    "react-fittext": "^1.0.0",
    "stylelint": "^13.0.0"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
