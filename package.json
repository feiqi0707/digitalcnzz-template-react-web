{
  "name": "{{name}}",
  "version": "1.1.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "ISC",
  "scripts": {
    "start": "cross-env API_ENV=dev umi dev",
    "start:test": "cross-env API_ENV=test umi dev",
    "build:dev": "cross-env API_ENV=dev umi build",
    "build:test": "cross-env API_ENV=test umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --colors --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },

  "dependencies": {
    "@amap/amap-jsapi-types": "^0.0.4",
    "@ant-design/pro-layout": "^5.0.9",
    "@ant-design/pro-table": "^2.2.7",
    "@ant-design/icons": "^4.2.2",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@umijs/hooks": "^1.9.2",
    "@umijs/preset-react": "^1.6.0",
    "@umijs/test": "^3.2.1",
    "react-router": "^5.2.0",
    "redbox-react": "^1.6.0",
    "dva-core": "^1.6.0-beta.7",
    "antd": "^4.6.0",
    "redux": "^4.0.0",
    "cross-env": "^7.0.2",
    "ms": "^2.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.0.3",
    "umi": "^3.2.0",
    "yorkie": "^2.0.0"
  },
  "__npminstall_done": false,
  "devDependencies": {
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "dayjs": "^1.9.0",
    "mockjs": "1.1.0",
    "ts-md5": "^1.2.7",
    "prettier": "^2.1.2",
    "postcss-px-to-viewport": "^1.1.1",
    "html2canvas": "^1.0.0-rc.7",
    "lint-staged": "^10.3.0"
  }
}
